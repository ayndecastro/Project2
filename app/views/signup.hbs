<body class="loginPage">
  <div class="col-md-7 pl-0">
    <img src="./public/styles/signup2.jpg">
  </div>
  <div class="container">

    <div class="row">

      <div class="col-md-5 mainText text-center">
        <div class="signupCenter">
          <h1>Sign-Up</h1>
          <form id="signup" name="signup" method="post" action="/signup"><br>
            <label for="email">Email Address</label><br>
            <input class="text" name="email" type="email" /><br>
            <label for="firstname">First Name</label><br>
            <input name="firstname" type="text" /><br>
            <label for="lastname">Last Name</label><br>
            <input name="lastname" type="text" /><br>
            <label for="password">Password</label><br>
            <input name="password" type="password" /><br>
            <input class="btn mt-3" style="background-color: #ad974f" type="submit" value="sign up" /><br>
          </form>
        </div>
        

      </div>
    </div>
  </div>
  <!-- <script type="text/javascript" src="/js/map.js"></script> -->
  <script type="text/javascript" src="./public/js/auth.js"></script>
</body>

</html>